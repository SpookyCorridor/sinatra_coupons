<h1>$$$ DA COUPONS $$$$</h1>

<ul>
  <li><a href='/coupons/create'>Create Coupon</a></li>
</ul>

<ul>
	<% @coupons.each do |coupon| %> 

	<li>
		<%= coupon.title.to_s + coupon.description.to_s + coupon.product.to_s + coupon.discount.to_s + coupon.expiration.to_s %> 
	</li>

	<li> <a href='/coupons/edit/<%= coupon.id %>'>Edit</a> </li>

<% end %> 
</ul>